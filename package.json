{
  "name": "generator-secure-data",
  "version": "1.0.0",
  "description": "Encrypt the secure data and wrap it into the module",
  "homepage": "https://github.com/oklas/generator-secure-data#readme",
  "author": {
    "name": "Serguei Okladnikov",
    "email": "oklaspec@gmail.com",
    "url": "https://github.com/oklas"
  },
  "repository": "https://oklas@github.com/oklas/generator-secure-data",
  "license": "MIT",
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": [
    "crypt",
    "encrypt",
    "secure",
    "data",
    "password",
    "module",
    "package",
    "application",
    "yeoman-generator"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^2.13.1",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^22.0.6"
  },
  "dependencies": {
    "async": "^2.6.0",
    "chalk": "^2.1.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  }
}
